{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "dist"
  },
  "typeRoots": [
    "./@types",
    "./node_modules/@types"
  ],
  "exclude": [
    "node_modules"
  ]
}
